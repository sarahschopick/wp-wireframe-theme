{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "title": "Wireframe",
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": true,
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "custom": true,
            "customGradient": true,
            "link": true,
            "palette": [
                {
                    "name": "Primary / Active",
                    "slug": "primary-active",
                    "color": "#5c8095"
                },
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#13587f"
                },
                {
                    "name": "Secondary / Active",
                    "slug": "secondary-active",
                    "color": "#22d3ee"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#0891b2"
                },
                {
                    "name": "Hover / Active",
                    "slug": "hover-active",
                    "color": "#06b6d4"
                },
                {
                    "name": "Hover",
                    "slug": "hover",
                    "color": "#0d9488"
                },
                {
                    "name": "Neutral / 950",
                    "slug": "neutral-950",
                    "color": "#15191c"
                },
                {
                    "name": "Neutral / 800",
                    "slug": "neutral-800",
                    "color": "#24272b"
                },
                {
                    "name": "Neutral / 600",
                    "slug": "neutral-600",
                    "color": "#3D4447"
                },
                {
                    "name": "Neutral / 400",
                    "slug": "neutral-400",
                    "color": "#DCD8D3"
                },
                {
                    "name": "Neutral / 200",
                    "slug": "neutral-200",
                    "color": "#F7EBE4"
                },
                {
                    "name": "Neutral / 0",
                    "slug": "neutral-0",
                    "color": "#FEF9F6"
                },
                {
                    "name": "Transparent",
                    "slug": "transparent",
                    "color": "Transparent"
                },
                {
                    "name": "Current",
                    "slug": "current",
                    "color": "currentcolor"
                },
                {
                    "name": "Inherit",
                    "slug": "inherit",
                    "color": "inherit"
                }
            ],
            "duotone": [
                {
                    "name": "Primary",
                    "slug": "primary",
                    "colors": [
                        "#5c8095",
                        "#13587f"
                    ]
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "colors": [
                        "#22d3ee",
                        "#0891b2"
                    ]
                },
                {
                    "name": "Hover",
                    "slug": "hover",
                    "colors": [
                        "#06b6d4",
                        "#0d9488"
                    ]
                }
            ],
            "gradients": [
                {
                    "name": "Primary",
                    "slug": "primary",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary-color) 0%, var(--wp--preset--color--primary-active) 100%)"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--secondary-active) 0%, var(--wp--preset--color--secondary-color) 100%)"
                },
                {
                    "name": "Hover",
                    "slug": "hover",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--hover-color) 0%, var(--wp--preset--color--hover-active) 100%)"
                },
                {
                    "name": "Primary / Secondary",
                    "slug": "primary-secondary",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary-color) 0%, var(--wp--preset--color--secondary-color) 100%)"
                },
                {
                    "name": "Primary / Secondary Active",
                    "slug": "primary-secondary-active",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary-active) 0%, var(--wp--preset--color--secondary-active) 100%)"
                },
                {
                    "name": "Current Color",
                    "slug": "current-color",
                    "gradient": "linear-gradient(135deg, transparent 0%, currentcolor 100%)"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textColumns": true,
            "writingMode": true,
            "fluid": true,
            "fontSizes": [
                {
                    "name": "Inherit",
                    "slug": "inherit",
                    "size": "inherit"
                },
                {
                    "name": "Extra Small",
                    "slug": "extra-small",
                    "size": "0.75rem"
                },
                {
                    "name": "Small",
                    "slug": "small",
                    "size": "0.0875rem"
                },
                {
                    "name": "Base",
                    "slug": "base",
                    "size": "1rem"
                },
                {
                    "fluid": {
                        "min": "1.05rem",
                        "max": "1.125rem"
                    },
                    "name": "Large",
                    "slug": "large",
                    "size": "clamp(1.05rem, 1rem + ((1vw - 0.2rem) * 0.16), 1.125rem)"
                },
                {
                    "fluid": {
                        "min": "1.125rem",
                        "max": "1.25rem"
                    },
                    "name": "Extra Large",
                    "slug": "x-large",
                    "size": "clamp(1.125rem, 1.125rem + ((1vw - 0.2rem) * 0.16), 1.25rem)"
                },
                {
                    "fluid": {
                        "min": "1.25rem",
                        "max": "1.5rem"
                    },
                    "name": "2X Large",
                    "slug": "2x-large",
                    "size": "clamp(1.25rem, 1.25rem + ((1vw - 0.2rem) * 0.31), 1.5rem)"
                },
                {
                    "fluid": {
                        "min": "1.5rem",
                        "max": "1.875rem"
                    },
                    "name": "3X Large",
                    "slug": "3x-large",
                    "size": "clamp(1.5rem, 1.5rem + ((1vw - 0.2rem) * 0.47), 1.875rem)"
                },
                {
                    "fluid": {
                        "min": "1.875rem",
                        "max": "2.25rem"
                    },
                    "name": "4X Large",
                    "slug": "4x-large",
                    "size": "clamp(1.875rem, 1.875rem + ((1vw - 0.2rem) * 0.47), 2.25rem)"
                },
                {
                    "fluid": {
                        "min": "2.25rem",
                        "max": "3rem"
                    },
                    "name": "5X Large",
                    "slug": "5x-large",
                    "size": "clamp(2.25rem, 2.25rem + ((1vw - 0.2rem) * 0.94), 3rem)"
                },
                {
                    "fluid": {
                        "min": "3rem",
                        "max": "3.75rem"
                    },
                    "name": "6X Large",
                    "slug": "6x-large",
                    "size": "clamp(3rem, 3rem + ((1vw - 0.2rem) * 0.94), 3.75rem)"
                },
                {
                    "fluid": {
                        "min": "3.75rem",
                        "max": "4.5rem"
                    },
                    "name": "7X Large",
                    "slug": "7x-large",
                    "size": "clamp(3.75rem, 3.75rem + ((1vw - 0.2rem) * 0.94), 4.5rem)"
                },
                {
                    "fluid": {
                        "min": "4.5rem",
                        "max": "6rem"
                    },
                    "name": "8X Large",
                    "slug": "8x-large",
                    "size": "clamp(4.5rem, 4.5rem + ((1vw - 0.2rem) * 1.88), 6rem)"
                },
                {
                    "fluid": {
                        "min": "6rem",
                        "max": "8rem"
                    },
                    "name": "9X Large",
                    "slug": "9x-large",
                    "size": "clamp(6rem, 6rem + ((1vw - 0.2rem) * 2.5), 8rem)"
                }
            ],
            "fontFamilies": [
                {
                    "name": "Inter",
                    "slug": "inter",
                    "fontFamily": "\"Inter\", sans-serif"
                },
                {
                    "name": "Cardo",
                    "slug": "cardo",
                    "fontFamily": "\"Cardo\", serif"
                },
                {
                    "name": "Bebas Neue",
                    "slug": "bebas-neue",
                    "fontFamily": "\"Bebas Neue\", \"Oswald\""
                },
                {
                    "name": "Dancing Script",
                    "slug": "dancing-script",
                    "fontFamily": "\"Dancing Script\", cursive"
                },
                {
                    "name": "Fira Code",
                    "slug": "fira-code",
                    "fontFamily": "\"Fira Code\", monospace"
                },
                {
                    "name": "System Sans Serif",
                    "slug": "system-sans-serif",
                    "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif"
                },
                {
                    "name": "System Serif",
                    "slug": "system-serif",
                    "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol"
                }
            ]
        },
        "spacing": {
            "defaultSpacingSizes": true,
            "customSpacingSize": true,
            "blockGap": true,
            "padding": true,
            "margin": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "spacingSizes": [
                {
                    "name": "2X Small",
                    "size": "0.5rem",
                    "slug": "2-x-small"
                },
                {
                    "name": "Extra Small",
                    "size": "1rem",
                    "slug": "x-small"
                },
                {
                    "name": "Small",
                    "size": "1.5rem",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "2rem",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "3rem",
                    "slug": "large"
                },
                {
                    "name": "Extra Large",
                    "size": "4rem",
                    "slug": "x-large"
                },
                {
                    "name": "2X Large",
                    "size": "6rem",
                    "slug": "2-x-large"
                }
            ]
        },
        "dimensions": {
            "minHeight": true
        },
        "layout": {
            "contentSize": "min(calc(100% - var(--wp--preset--spacing--xl,5%)), 720px)",
            "wideSize": "min(calc(100% - var(--wp--preset--spacing--xl,4rem)), 1280px)"
        },
        "position": {
            "sticky": true
        },
        "blocks": {
            "core/image": {
                "appearanceTools": true,
                "lightbox": {
                    "enabled": false,
                    "allowEditing": true
                }
            },
            "core/post-terms": {
                "spacing": {
                    "blockGap": true
                }
            }
        },
        "custom": {
            "fontFamily": {
                "body": "var(--wp--preset--font-family--inter)",
                "heading": "var(--wp--preset--font-family--cardo)",
                "mono": "var(--wp--preset--font-family--fira-code)"
            },
            "color": {
                "body": "var(--wp--preset--color--neutral-0)",
                "heading": "var(--wp--preset--color--neutral-950)",
                "text": "var(--wp--preset--color--neutral-800)",
                "primary": "var(--wp--preset--color--primary-color)",
                "active": "var(--wp--preset--color--primary-active)"
            },
            "fontSize": {
                "body": "var(--wp--preset--font-size--large)",
                "h1": "var(--wp--preset--font-size--6-x-large)",
                "h2": "var(--wp--preset--font-size--5-x-large)",
                "h3": "var(--wp--preset--font-size--4-x-large)",
                "h4": "var(--wp--preset--font-size--3-x-large)",
                "h5": "var(--wp--preset--font-size--2-x-large)",
                "h6": "var(--wp--preset--font-size--large)"
            },
            "border": {
                "color": "var(--wp--preset--color--neutral-400)",
                "style": "solid",
                "width": "1px"
            },
            "borderRadius": {
                "none": "0px",
                "small": "0.125rem",
                "rounded": "0.25rem",
                "medium": "0.375rem",
                "large": "0.5rem",
                "x-large": "0.75rem",
                "2-x-large": "1rem",
                "3-x-large": "1.5rem",
                "full": "9999px"
            },
            "fontWeight": {
                "black": 900,
                "extraBold": 800,
                "bold": 700,
                "semiBold": 600,
                "medium": 500,
                "regular": 400,
                "light": 300,
                "extraLight": 200,
                "thin": 100
            },
            "transition": "all ease-in 0.15s",
            "elements": {
                "link": {
                    "textUnderlineOffset": "0.35em",
                    "textDecoration": "underline solid 0.08em",
                    ":hover": {
                        "textUnderlineOffset": "0.35em",
                        "textDecoration": "underline dashed 0.08em"
                    }
                },
                "label": {
                    "typography": {
                        "fontSize": "var(--wp--preset--font-size--small)"
                    }
                },
                "checkbox": {
                    "borderWidth": "1px",
                    "background": "var(--wp--preset--color--neutral-200)"
                },
                "input": {
                    "padding": {
                        "top": "0.65rem",
                        "bottom": "0.65rem",
                        "left": "1rem",
                        "right": "1rem"
                    },
                    "borderWidth": "0",
                    "background": "var(--wp--preset--color--neutral-200)",
                    ":focus": {
                        "background": "var(--wp--preset--color--neutral-200)",
                        "borderColor": "var(--wp--custom--color--primary)"
                    }
                },
                "button": {
                    "color": {
                        "text": "var(--wp--preset--color--neutral-950)",
                        "background": "var(--wp--preset--color--neutral-0)"
                    },
                    "hover": {
                        "color": {
                            "text": "var(--wp--preset--color--neutral-950)",
                            "background": "var(--wp--preset--color--neutral-600)"
                        }
                    }
                }
            },
            "header": {
                "zIndex": "2"
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--custom--color--body)",
            "text": "var(--wp--custom--color--text)"
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--small)"
        },
        "typography": {
            "fontFamily": "var(--wp--custom--font-family--body)",
            "fontSize": "var(--wp--custom--font-size--body)",
            "fontWeight": "var(--wp--preset--font-weight--regular)",
            "lineHeight": "1.4"
        },
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--custom--font-family--heading)",
                    "fontWeight": "var(--wp--custom--font-weight--semi-bold)",
                    "lineHeight": "1.4"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-1)",
                    "lineHeight": "1.15"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-2)",
                    "lineHeight": "1.15"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-3)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-5)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-size--h-6)"
                }
            },
            "link": {
                "typography": {
                    "textDecoration": "var(--wp--custom--elements--link--text-decoration)"
                },
                "color": {
                    "text": "inherit"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--custom--color--primary)"
                    },
                    "typography": {
                        "textDecoration": "var(--wp--custom--elements--link--hover--text-decoration)"
                    }
                },
                ":focus": {
                    "color": {
                        "text": "var(--wp--custom--color--primary)"
                    },
                    "typography": {
                        "textDecoration": "var(--wp--custom--elements--link--hover--text-decoration)"
                    }
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--custom--elements--button--color--background, var(--wp--custom--color--primary, currentColor))"
                }
            }
        },
        "blocks": {
            "core/buttons": {
                "color": {
                    "text": "var(--wp--custom--color--text)"
                },
                "border": {
                    "radius": "var(--wp--custom--border-radius--rounded)"
                },
                "spacing": {
                    "blockGap": "var(--wp--preset--spacing--x-small)"
                }
            },
            "core/button": {
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--x-small)",
                        "right": "var(--wp--preset--spacing--small)",
                        "bottom": "var(--wp--preset--spacing--x-small)",
                        "left": "var(--wp--preset--spacing--small)"
                    },
                    "blockGap": "var(--wp--preset--spacing--2-x-small)"
                },
                "variations": {
                    "outline": {
                        "spacing": {
                            "padding": {
                                "top": "var(--wp--preset--spacing--x-small)",
                                "right": "var(--wp--preset--spacing--small)",
                                "bottom": "var(--wp--preset--spacing--x-small)",
                                "left": "var(--wp--preset--spacing--small)"
                            },
                            "blockGap": "var(--wp--preset--spacing--2-x-small)"
                        },
                        "border": {
                            "color": "currentColor",
                            "width": "2px"
                        }
                    }
                }
            },
            "core/quote": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "color": {
                    "background": "var(--wp--preset--color--neutral-200)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontFamily": "var(--wp--preset--font-family--inter)",
                            "fontSize": "var(--wp--preset--font-size--base)",
                            "fontStyle": "normal"
                        }
                    }
                },
                "spacing": {
                    "padding": {
                        "bottom": "calc(var(--wp--preset--spacing--small) + 0.75rem)",
                        "left": "calc(var(--wp--preset--spacing--small) + 0.75rem)",
                        "right": "calc(var(--wp--preset--spacing--small) + 0.75rem)",
                        "top": "calc(var(--wp--preset--spacing--small) + 0.75rem)"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--cardo)",
                    "fontSize": "var(--wp--preset--font-size--3-x-large)",
                    "fontStyle": "italic",
                    "lineHeight": "1.3"
                },
                "variations": {
                    "plain": {
                        "border": {
                            "color": "var(--wp--preset--color--neutral-600)",
                            "radius": "0",
                            "style": "solid",
                            "width": "0"
                        },
                        "color": {
                            "background": "transparent"
                        },
                        "spacing": {
                            "padding": {
                                "bottom": "var(--wp--preset--spacing--small)",
                                "left": "var(--wp--preset--spacing--small)",
                                "right": "var(--wp--preset--spacing--small)",
                                "top": "var(--wp--preset--spacing--small)"
                            }
                        },
                        "typography": {
                            "fontFamily": "var(--wp--preset--font-family--inter)",
                            "fontStyle": "normal",
                            "fontSize": "var(--wp--preset--font-size--x-large)",
                            "lineHeight": "1.5"
                        }
                    }
                }
            },
            "core/pullquote": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontFamily": "var(--wp--preset--font-family--inter)",
                            "fontSize": "var(--wp--preset--font-size--base)",
                            "fontStyle": "normal"
                        }
                    }
                },
                "color": {
                    "background": "var(--wp--preset--color--neutral-200)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "var(--wp--preset--spacing--large)",
                        "top": "var(--wp--preset--spacing--large)",
                        "right": "var(--wp--preset--spacing--medium)",
                        "left": "var(--wp--preset--spacing--medium)"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--cardo)",
                    "fontSize": "var(--wp--preset--font-size--4-x-large)",
                    "fontStyle": "italic",
                    "fontWeight": "400",
                    "letterSpacing": "0em",
                    "lineHeight": "1.5"
                }
            },
            "core/code": {
                "color": {
                    "background": "var(--wp--preset--color--neutral-800)",
                    "text": "var(--wp--preset--color--neutral-200)"
                },
                "border": {
                    "color": "var(--wp--preset--color--neutral-600)",
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "typography": {
                    "fontFamily": "var(--wp--custom--font-family--mono)",
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--small)",
                        "right": "var(--wp--preset--spacing--small)",
                        "bottom": "var(--wp--preset--spacing--small)",
                        "left": "var(--wp--preset--spacing--small)"
                    }
                }
            },
            "core/preformatted": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "typography": {
                    "fontFamily": "var(--wp--custom--font-family--mono)",
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "fontWeight": "var(--wp--custom--font-weight--regular)"
                }
            },
            "core/details": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "color": {
                    "background": "var(--wp--preset--color--neutral-200)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--x-small)",
                        "right": "var(--wp--preset--spacing--small)",
                        "bottom": "var(--wp--preset--spacing--x-small)",
                        "left": "var(--wp--preset--spacing--small)"
                    }
                }
            },
            "core/calendar": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-small)"
                }
            },
            "core/list": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "spacing": {
                    "padding": {
                        "left": "var(--wp--preset--spacing--medium)"
                    }
                }
            },
            "core/image": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "variations": {
                    "rounded": {
                        "border": {
                            "radius": "var(--wp--custom--border-radius--full)"
                        }
                    }
                }
            },
            "core/video": {
                "border": {
                    "radius": "var(--wp--custom--border-radius--2-x-large)"
                },
                "color": {
